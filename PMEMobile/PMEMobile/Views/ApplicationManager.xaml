<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PMEMobile.Views.ApplicationManager">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="Show Current Apps" Clicked="ShowApp"/>
            <Button Text="Select Apps" Clicked="SelectApp"/>
            <Button Text="Add Limit" Clicked="LimitApp"/>
            <Button Text="Clear Usage" Clicked="ClearUsage" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>